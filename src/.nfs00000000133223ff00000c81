#! bin/bash 

python run.neighbor.py --candidate_enhancer_regions H3K27ac_LCL_candidate_regions.txt --genes /oak/stanford/groups/akundaje/projects/kristy_datasets/Enhancer_Gene_QTLs/GENCODE.v19.annotation.csv --H3K27ac /mnt/lab_data2/kmualim/data/send_to_Kristy/GM12878_Files/ENCFF197QHX_H3K27ac_rep1.bam --DHS /mnt/lab_data2/kmualim/data/send_to_Kristy/GM12878_Files/ENCFF775ZJX_DNase_rep1.bam --chrom_sizes /mnt/lab_data2/kmualim/data/send_to_Kristy/hg19.chrom.sizes --ubiquitously_expressed_genes /users/kmualim/updated_ABC/ABC-Enhancer-Gene-Prediction/example/config/UbiquitouslyExpressedGenesHG19.txt --cellType GM12878 --outdir /mnt/lab_data2/kmualim/modified_ABC/results/QTL_results/Neighborhoods_H3K27ac_peak_LCL_QTL/

