#! /bin/bash 


preddir=CRISPR
exptdata=data/ExperimentalData.Gasperini.FulcoNasser.191021.txt

echo "Running Comparison Script"
for i in {1..10}
do 
	echo "Running file"
	echo $i
	Rscript comparePredictionsToExperiment.R --predictions train_$i--experimentalData $exptdata --plotConfig $preddir/plot.config.txt --predConfig $preddir/pred.config.txt
	bash renameFiles.sh train_$i
done
