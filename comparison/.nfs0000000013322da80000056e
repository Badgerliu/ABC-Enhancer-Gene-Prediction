#! /bin/bash 

outdir=$1
preddir=CRISPR
exptdata=data/ExperimentalData.Gasperini.FulcoNasser.191021.txt

echo "Running Comparison Script"
for i in {0..9}
do 
	echo "Running file"
	echo $i
	Rscript comparePredictionsToExperiment.R --predictions $outdir/train_$i --experimentalData $exptdata --plotConfig $preddir/plot.config.txt --predConfig $preddir/pred.config.txt
	bash renameFiles.sh train_$i $outdir
done
